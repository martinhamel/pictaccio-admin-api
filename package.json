    "typedoc": "^0.25.3",
    "typescript": "^5.2.2",
    "uglify-js": "^3.17.4",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.11",
    "class-validator": "*",
