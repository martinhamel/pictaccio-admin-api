{
  "name": "@pictaccio/admin-api",
  "version": "0.0.5",
  "description": "API service for admin app",
  "main": "src/entry.ts",
  "scripts": {
    "serve": "nodemon --watch src --ext ts --exec \"tsc && node dist/entry.js\"",
    "serve:backend": "docker compose -f build/dev-backend-compose.yaml up",
    "stop:backend": "docker compose -f build/dev-backend-compose.yaml down",
    "build": "npx tspc",
    "main-build": "echo Compiling backend (main-build)... && npx tspc",
    "post-compile": "echo Running post compile tasks... && npx gulp postCompileTasks",
    "post-compile-prod": "echo Running post compile tasks... && npx gulp postCompileTasksProd",
    "build:docker:alpha": "npx gulp buildDockerAlpha",
    "build:docker:beta": "npx gulp buildDockerBeta",
    "build:docker:prod": "npx gulp buildDockerProd",
    "build:docker:winter24": "npx gulp buildDockerWinter24",
    "build:docker:test": "npx gulp buildDockerTest",
    "fake-it": "NODE_PATH=./src/ ts-node build/fake_it.ts",
    "test": "NODE_PATH=./src/ NODE_ENV=test mocha -r ts-node/register test/unit/**/*.test.ts",
    "coverage": "NODE_PATH=./src/ NODE_ENV=test nyc mocha -r ts-node/register --fail-zero test/unit/**/*.test.ts",
    "lint": "eslint src/ --ext .ts --quiet",
    "typeorm": "NODE_PATH=./src/ node --require ts-node/register --require tsconfig-paths/register build/typeorm_init.ts",
    "typeorm-compiled": "NODE_PATH=./src/ node build/typeorm_init.js",
    "cloc": "cloc src --by-file-by-lang"
  },
  "private": true,
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/body-parser": "^1.19.4",
    "@types/express": "^5.0.2",
    "@types/express-session": "^1.17.9",
    "@types/js-yaml": "^4.0.8",
    "@types/multer": "^1.4.9",
    "@types/node": "^20.8.10",
    "@types/node-cron": "^3.0.10",
    "@types/sinon": "^17.0.0",
    "@types/sinon-chai": "^3.2.11",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "chai": "^4.3.10",
    "cloc": "^2.11.0",
    "enforce-unique": "^1.3.0",
    "eslint": "^8.53.0",
    "google-closure-compiler": "^20230802.0.0",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-strip-comments": "^2.6.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-util": "^3.0.8",
    "immutable": "^4.0.0",
    "jsdom": "^22.1.0",
    "mitt": "^3.0.1",
    "mocha": "^10.2.0",
    "mysql": "^2.18.1",
    "node-fetch": "^2.7.0",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "nodemon": "^3.0.1",
    "nyc": "^15.1.0",
    "pure-rand": "^6.1.0",
    "requirejs": "^2.3.6",
    "sass": "^1.69.5",
    "sinon": "^17.0.1",
    "sinon-chai": "^3.7.0",
    "ts-mock-imports": "^1.3.8",
    "ts-node": "^10.9.1",
