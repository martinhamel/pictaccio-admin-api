    "@loufa/routing-controllers": "*",
    "@loufa/routing-controllers-openapi": "*",
    "@pictaccio/admin-api": "*",
    "@sendgrid/mail": "^7.7.0",
    "accesscontrol": "^2.2.1",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
